{
  "name": "html-mail-sender",
  "version": "1.0.4",
  "description": "HtmlMailSender is a TypeScript class for sending HTML emails using Nodemailer and Handlebars. This class dynamically generates the email content using a specified HTML template and sends it to specified recipients.",
  "main": "index.js",
  "type": "commonjs",
  "types": "./index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --poll index.ts",
    "build": "ts-node --esm",
    "build-ex": "tsc"
  },
  "keywords": [
    "netGSM",
    "integration",
    "sms",
    "phone",
    "number",
    "message"
  ],
  "author": "Furkan Ilhan(murki)",
  "license": "ISC",
  "dependencies": {
    "handlebars": "^4.7.8",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.1",
    "@types/nodemailer": "^6.4.14",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}